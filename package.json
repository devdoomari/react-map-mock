{
  "name": "react-daum-map",
  "version": "0.0.1",
  "description": "React wrapper for daum map",
  "main": "lib/index.js",
  "typescript": {
    "definition": "index.d.ts"
  },
  "scripts": {
    "build": "node_modules/.bin/webpack --config examples/webpack_config/build.js --bail -p",
    "clean": "rm -r examples/dist dist",
    "watch_examples": "node_modules/.bin/webpack --watch --config examples/webpack_config/build.js --bail -p",
    "dev": "node_modules/.bin/webpack-dev-server --config examples/webpack_config/config.js --history-api-fallback",
    "_serve_examples": "$(npm bin)/webpack-dev-server --config examples/webpack.config.js --port 8081",
    "_watch_ts_for_examples": "$(npm bin)/tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdoomari/react-daum-map.git"
  },
  "keywords": [
    "react",
    "daum",
    "map"
  ],
  "author": "devdoomari@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devdoomari/react-daum-map/issues"
  },
  "homepage": "https://github.com/devdoomari/react-daum-map#readme",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "core-decorators": "^0.12.2",
    "keymirror": "^0.1.1",
    "lodash": "^4.9.0",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-jest": "^10.0.1",
    "babel-polyfill": "^6.7.4",
    "codemirror": "^5.9.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "jest-cli": "^0.10.0",
    "postscribe": "^2.0.1",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "^0.28.1",
    "react-modular-map": "git+ssh://git@github.com/devdoomari/react-map.git#dist_35",
    "typings": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "jest": {
    "testFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ]
  }
}
